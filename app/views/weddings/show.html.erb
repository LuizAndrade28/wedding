<section class="testbwed-red">
  <%= render partial: "navbar" %>
  <%= render 'banner_wed' %>
    <section class="buffer-wed"></section>
    <section class="test-red-bg"></section>
    <div class="container test-blur-wed">
      <div class="row">
        <div class="col-md-12">
          <div class="d-flex flex-column align-items-center" data-bs-spy="scroll" data-bs-target="#anchors-nav-wed" data-bs-smooth-scroll="true" class="scrollspy-example" tabindex="0">
            <%= render 'welcome_message' %>
            <section class="buffer-wed"></section>
            <section class="parallax-wed"></section>
            <section class="buffer-wed"></section>
            <%= render "confirmation" %>
            <section class="buffer-wed"></section>
            <%= render "events" %>
            <section class="buffer-wed"></section>
            <%= render "tips" %>
            <section class="buffer-wed"></section>
            <% if !@wedding_guests_messages.empty? %>
              <%= render 'messages' %>
            <% end %>
            <section class="buffer-wed"></section>
            <%= render 'final_message' %>
            <section class="buffer-wed"></section>
            </div>
        </div>
      </div>
    </div>
  <%= render 'footer' %>
  <section class="buffer-wed"></section>
</section>
